# Menü Aç
dialog show @p[tag=gulce_admin] {  "type": "minecraft:multi_action",  "title": "Eylem Yöneticisi",  "body": {    "type": "minecraft:plain_message",    "contents": "Eylem Yönetimi:\n1. Eylem Sil\n2. Eylem Ekle"  },  "can_close_with_escape": true,  "pause": false,  "columns": 1,  "actions": [    {      "label": "Eylem Ekle",      "tooltip": "Tıkla ve Kopyala:\n/function custom_admin:add/action_\nÖrnekler:\n1:\n/function custom_admin:add/action_command {id:0,command:\"say Test\",player:\"@p\"}\n2:\n/function custom_admin:add/action_give {id:0,item:\"paper\",player:\"@p\",count:1,nbt:'[custom_data={test:1b}]'}\n3:\n/function custom_admin:add/action_message {id:0,player:\"@p\",text:\"Test\"}",      "action": {        "type": "minecraft:copy_to_clipboard",        "value": "/function custom_admin:add/action_"      }    },    {      "label": "Eylem Sil",      "action": {        "type": "minecraft:show_dialog",        "dialog": {          "type": "minecraft:multi_action",          "title": "",          "inputs": [            {              "type": "minecraft:text",              "key": "arg",              "label": "ID",              "max_length": 100000,              "multiline": {}            }          ],          "can_close_with_escape": true,          "pause": false,          "after_action": "close",          "actions": [            {              "label": "Sil",              "action": {                "type": "minecraft:dynamic/run_command",                "template": "/function custom_admin:remove/action {id:\"$(arg)\"}"              }            }          ]        }      }    }  ]}

# Başarı
tellraw @a[tag=gulce_admin,predicate=custom_admin:ui] ["",{"text":"[GULCE] ","color":"gold","bold":true},{"text":"Eylem yönetimi yüklendi.","color":"gray"}]

# Hata
execute if entity @s[tag=!gulce_admin] run tellraw @s {"text":"Yetkin Yok!"}